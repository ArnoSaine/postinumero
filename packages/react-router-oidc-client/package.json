{
  "name": "@postinumero/react-router-oidc-client",
  "version": "0.4.0",
  "description": "A React Router integration for OpenID Connect (OIDC) authentication, built on top of oidc-client-ts.",
  "keywords": [
    "react-router",
    "auth",
    "oidc",
    "keycloak",
    "rbac"
  ],
  "homepage": "https://github.com/ArnoSaine/postinumero/tree/main/packages/react-router-oidc-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArnoSaine/postinumero.git",
    "directory": "packages/react-router-oidc-client"
  },
  "license": "ISC",
  "author": {
    "name": "Arno Saine",
    "email": "arno@mowhi.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./client/hooks/*": "./lib/client/hooks/*.js",
    "./client/loaders/*": "./lib/client/loaders/*.js",
    "./client/*": "./lib/client/*.js",
    "./components": "./lib/components/index.js",
    "./components/*": "./lib/components/*.js",
    "./hooks/*": "./lib/hooks/*.js",
    "./keycloak": "./lib/keycloak/index.js",
    "./keycloak/loaders/*": "./lib/keycloak/loaders/*.js",
    "./keycloak/routes/*": "./lib/keycloak/routes/*.js",
    "./keycloak/*": "./lib/keycloak/*.js",
    "./loaders/*": "./lib/loaders/*.js",
    "./routes/*": "./lib/routes/*.js",
    "./server/*": "./lib/server/*.js",
    "./token/*": "./lib/token/*.js",
    "./user/client/*": "./lib/user/client/*.js",
    "./user/server/*": "./lib/user/server/*.js",
    "./user/*": "./lib/user/*.js",
    "./utils/react-router/*": "./lib/utils/react-router/*.js",
    "./utils/*": "./lib/utils/*.js",
    "./*": "./lib/*.js"
  },
  "files": [
    "lang",
    "lib",
    "src"
  ],
  "scripts": {
    "prebuild": "formatjs-tools extract --path src",
    "build": "tspc --project tsconfig.lib.json",
    "predev": "npm run prebuild",
    "dev": "npm run build -- --watch",
    "typecheck": "npm run build --ignore-scripts -- --noEmit"
  },
  "dependencies": {
    "@arnosaine/is": "^0.2.9",
    "assert-response": "^0.2.1",
    "flat": "^6.0.1",
    "jose": "^6.0.10",
    "lodash-es": "^4.17.21",
    "oidc-client-ts": "^3.1.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@postinumero/formatjs-tools": "^0.1.6",
    "@types/node": "^24.0.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@react-router/dev": ">=7.9.1",
    "react-intl": ">=7.0.0",
    "react-router": ">=7.9.1"
  }
}
