{
  "name": "@postinumero/react-auth",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./lib/main.js",
    "./Auth": "./lib/Auth.js",
    "./Context": "./lib/Context.js",
    "./HasRole": "./lib/HasRole.js",
    "./IsAllowed": "./lib/IsAllowed.js",
    "./IsAuthenticated": "./lib/IsAuthenticated.js",
    "./useCurrentUser": "./lib/useCurrentUser.js",
    "./useHasRole": "./lib/useHasRole.js",
    "./useIsAllowed": "./lib/useIsAllowed.js",
    "./useIsAuthenticated": "./lib/useIsAuthenticated.js",
    "./withAuth": "./lib/withAuth.js"
  },
  "main": "./lib/main.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:app": "react-scripts build",
    "build:lib": "library-scripts build",
    "eject": "react-scripts eject",
    "prepare": "npm run build:lib",
    "prepublishOnly": "npm run build:lib && cross-env CI=true npm run testOnly",
    "start": "concurrently \"npm run watch\" \"react-scripts start\"",
    "startOnly": "react-scripts start",
    "test": "concurrently \"npm run watch\" \"react-scripts test\"",
    "testOnly": "react-scripts test",
    "watch": "library-scripts watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "@postinumero/react-auth": "<rootDir>"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "devDependencies": {
    "@arnosaine/react-scripts": "^4.0.0-3",
    "@postinumero/library-scripts": "^0.1.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
