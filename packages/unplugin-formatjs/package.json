{
  "name": "@postinumero/unplugin-formatjs",
  "type": "module",
  "version": "0.1.5",
  "description": "Preconfigured. Minimal setup.",
  "license": "ISC",
  "homepage": "https://github.com/ArnoSaine/postinumero/tree/main/packages/unplugin-formatjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArnoSaine/postinumero.git",
    "directory": "packages/unplugin-formatjs"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "formatjs"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-syntax-typescript": "^7.27.1",
    "@formatjs/ts-transformer": "^3.13.34",
    "@postinumero/formatjs-tools": "^0.1.3",
    "@swc/core": "^1.11.21",
    "@swc/plugin-formatjs": "^3.2.1",
    "babel-plugin-formatjs": "^10.5.37",
    "safe-stable-stringify": "^2.5.0",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "eslint": "^9.20.0",
    "nodemon": "^3.1.9",
    "rollup": "^4.34.6",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "webpack": "^5.97.1"
  }
